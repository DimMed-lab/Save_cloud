@model List<Save_cloud.Models.Users>

@foreach (var user in Model)
{
    <div class="user-item">
        <div class="form-check">
            <input type="checkbox" id="user_@user.Id" name="selectedUsers" value="@user.Id" class="form-check-input" />
            <label for="user_@user.Id" class="form-check-label">@user.Username</label>
        </div>
    </div>
}